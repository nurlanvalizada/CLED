@model CledAcademy.Core.Domain.DataDictionary
@{
    ViewBag.Title = "İstifadəçi Razılaşması";
}

 <!-- Start main-content -->
<div class="main-content">
  
    <!-- Section: inner-header -->
    <section class="inner-header divider parallax layer-overlay overlay-dark-5" data-bg-img="/images/bg3.jpg">
        <div class="container pt-70 pb-20">
            <!-- Section Content -->
            <div class="section-content">
                <div class="row">
                    <div class="col-md-12">
                        <h2 class="title text-white">İstifadəçi Razılaşması</h2>
                        <ol class="breadcrumb text-left text-black mt-10">
                            <li><a href="http://www.cledacademy.com">CledAcademy.Com</a></li>
                            <li><a asp-controller="Home" asp-action="Index">Ana Səhifə</a></li>
                            <li class="active text-gray-silver">İstifadəçi Razılaşması</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Section: Blog -->
    <section>
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <div class="blog-posts single-post">
                        <article class="post clearfix mb-0">
                            <div class="entry-title">
                                <h4><a class="text-uppercase">@Model.Header</a></h4>
                            </div>
                            <div class="entry-content mt-10">
                                @Html.Raw(Model.Value)
                            </div>
                        </article>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
<!-- end main-content -->